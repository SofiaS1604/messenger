.form__button
  outline none
  height 40px
  text-align center
  width 130px
  border-radius 40px
  background #fff
  border 2px solid #1ECD97
  color #1ECD97
  letter-spacing 1px
  font-size 16px
  cursor pointer
  transition all 0.25s ease
  margin-left auto
  margin-right: auto;
  margin-top 35px
  &:hover
    color white;
    background #1ECD97